#CCFLAGS_USER += usrflg2, ex -g0

#CFLAGS_USER  = usrflg1, ex -DFOO
#CFLAGS_USER += usrflg2, ex -o1

#LDFLAGS_USER  = usrflg1, ex: -static, only exemake
#LDFLAGS_USER += usrflg2, ex: -s, only exemake

